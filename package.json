{
  "name": "node-docker-webpack-starter",
  "version": "1.0.0",
  "description": "Boilerplate for node app using webpack and optional docker setup. Setup for dev and prod.",
  "scripts": {
    "start": "babel-node server/devServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node scripts/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node server/distServer.js",
    "create": "babel-node scripts/createNewStarter.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "npm-run-all": "^4.0.1",
    "open": "0.0.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0"
  }
}
